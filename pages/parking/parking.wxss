/**index.wxss**/
.parking {
  background: #f9f9f9;
  height: 100%;
  width:100%;
}
.searchbar {
  position: relative;
  height: 80rpx;
  padding:28rpx;
  overflow: hidden;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
  background: #30DDD9;
}
.searchbar .search-input {
  position:relative;
  border-radius: 40rpx;
  background: rgba(4,4,4,0.08);
}
.searchbar .search-input input {
  height: 80rpx;
  padding-left: 84rpx;
  font-size: 28rpx;
  position: relative;
  z-index: 1;
  border-radius: 40rpx;
}
.searchbar .search-input .search-icon {
  position: absolute;
  top:18rpx; 
  left:40rpx;
  width:45rpx;
  height:45rpx;
  opacity: 1;
  z-index: 2;
}
.searchbar .searchbar-cancel {
  float: right;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  -webkit-transition: all .3s;
  transition: all .3s;
  opacity: 0;
  -webkit-transform: translate3d(0, 0, 0);
  transform: translate3d(0, 0, 0);
}
.searchbar.searchbar-active .searchbar-cancel + .search-input {
  margin-right: 80rpx;
}
.searchbar.searchbar-active .searchbar-cancel {
    margin-right: 0;
    opacity: 1;
}

.depot-list {
  padding:30rpx;
}
.depot-list .depot-box {
  background: #fff;
  padding: 36rpx;
  margin-bottom: 26rpx;
  box-shadow: 0 2px 4px 0 rgba(0,0,0,0.02);
  border-radius: 8px;
}
.depot-list .depot-box .depot-hd {
  height: 60rpx;
  line-height: 60rpx;
  font-size: 0;
  vertical-align: bottom;
}
.depot-list .depot-box .depot-hd .depot-title {
  font-size:32rpx;
  color:#333;
  display: inline-block;
}
.depot-list .depot-box .depot-hd .range {
  font-size:18rpx;
  color:#666;
  display: inline-block;
  margin-left: 32rpx;
}
.depot-list .depot-box .depot-seat {
  font-size: 26rpx;
  height: 60rpx;
  line-height: 60rpx;
  color:#9b9b9b;
  padding-bottom: 42rpx;
}
.depot-list .depot-box .depot-seat image {
  height: 26rpx;
  width:20rpx;
  vertical-align: 0rpx;
  margin-right: 10rpx;
}
.depot-list .depot-box .depot-info {
  display: -webkit-box;
  display: -moz-box;
  display: -ms-flexbox;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -moz-justify-content: space-between;
  -webkit-justify-content: space-between;
  justify-content: space-between;
  -webkit-box-align: center;
  -moz-align-items: center;
  -webkit-align-items: center;
  align-items: center;
  font-size: 26rpx;
  height: 60rpx;
  padding: 0 20rpx;
}
.depot-list .depot-box .depot-info.h100 {
  height: 100rpx;
}
.depot-list .depot-box .depot-info .hot {
  color:#000;
  margin-left: 18rpx;
}
.depot-list .depot-box .depot-info .default {
  color:#a5a5a5;
}
.depot-list .depot-box .depot-info .price {
  color:#a5a5a5;
  display: none;
}
.depot-list .depot-box .depot-info .price .red {
  color:#f25b57;
  margin-left: 18rpx;
}
.depot-list .depot-box .depot-info.h100 .price{
  display: block;
}
.depot-list .no-more {
  color:#868686;
  text-align: center;
  font-size:24rpx;
  height: 40rpx;
  line-height: 40rpx;
}
.parking #reserve {
  position: fixed;
  bottom:52rpx;
  right:30rpx;
}
.parking #reserve image {
  width:134rpx;
  height:134rpx;
}
 .unclock {
  position: relative;
  line-height: 80rpx;
} 
.unclock image {
  width: 80rpx;
  height: 80rpx;
  vertical-align: middle;
}
