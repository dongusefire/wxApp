/* pages/myTrip/myTrip.wxss */
.parent {
  background: #fbfbfb;
}
.column{
  background:#ffffff;
  margin-bottom: 30rpx;
}
.column .park-title{
  height: 98rpx;
  padding:0 36rpx;
  display: -webkit-box;
  display: -moz-box;
  display: -ms-flexbox;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -moz-justify-content: space-between;
  -webkit-justify-content: space-between;
  justify-content:space-between;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
}
.column .park-title .car-stop text{
   font-size:30rpx;
   color:#333;

}
.column .park-title .deal-btn .pay {
    font-size:24rpx;
    color:#ff5454;
}
.column .park-title .deal-btn .pay.finished {
  color:#9b9b9b;
  font-size:24rpx;
}
.park-info {
  padding:0 34rpx;
  border-top:2rpx solid #ECECEC;
  font-size: 26rpx;
  color:#9b9b9b;
  border-bottom:2rpx solid #ECECEC;
  padding-bottom: 18rpx;
}
.park-pos{
    padding-top:18rpx;
    display: -webkit-box;
    display: -moz-box;
    display: -ms-flexbox;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    -webkit-align-items: center;
    align-items: center;
}
.park-pos image{
  width:20rpx;
  margin-right: 10rpx;
}
.order-time{
  padding-top: 12rpx;
}
.order-time .time .data{
    padding-left:16rpx;
}
.car-num{
    padding-top: 18rpx;
}
.car-num .time{
   color:#949494;
}
.car-num .cost{
  padding-right:40rpx;
}
.car-num .time .data{
    padding-left:18rpx;
}
.park-info .price {
  padding-top: 34rpx;
  font-size: 26rpx;
  color:#373737;
}
.park-info .price .data {
  padding-left: 20rpx;
}
.details {
  display: flex;
  display: -webkit-flex;
  display: -webkit-box;
  display: -moz-box;
  display: -ms-flexbox;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
  -webkit-box-pack: end;
  -moz-justify-content: flex-end;
  -webkit-justify-content: flex-end;
  justify-content: flex-end;
  height: 96rpx;
  padding-right: 34rpx;
  font-size: 26rpx;
  color:#333;
}
.bar {
  height: 28rpx;
  background-color: rgba(204, 204, 204, 1);
  padding: 0;
}
.reminder {
  background-color: #fff;
  text-align: center;
  color:#949494;
  height: 100rpx;
  line-height: 100rpx;
}