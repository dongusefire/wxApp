/* pages/regs/regs.wxss */
.reg-container{
  border-top:2rpx solid #c9c9c9;
  margin-top:100rpx;
}
.form{
  padding:0 30rpx 0 30rpx;
}
.itemView{
  height:90rpx;
  border-bottom:2rpx solid #c9c9c9;
}
.itemView input{
  height:90rpx;
  font-size:32rpx;
  color:#999999;
}
.code{
  display: -webkit-box;
  display: -moz-box;
  display: -ms-flexbox;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
}
.code input{
  flex:1;
}
.code .btn{
  background: none;
  border:none;
  height:68rpx;
  background: #ff6633;
  font-size:26rpx;
  color:#ffffff;
  line-height:68rpx;
  text-align: center;
}
.code .btn.count {
  background-color: #dcdcdc;
  border:none;
  outline: none;
}
.code .btn.count:after {
  content: initial;
}
.key{
  width:100%;
  display: -webkit-box;
  display: -moz-box;
  display: -ms-flexbox;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
}
.key input{
  flex:1;
}
.key .key-img{
  width:64rpx;
  height:64rpx;
  padding-left: 100rpx;
}
.key .key-img image{
  width:100%;
  height:100%;
}
.joinus{
   margin-top:42rpx;
}
.joinus{
  display: -webkit-box;
  display: -moz-box;
  display: -ms-flexbox;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
  -webkit-box-pack: justify;
  -moz-justify-content: center;
  -webkit-justify-content: center;
  justify-content:center;
}

.joinus text{
 font-size:30rpx;
 color:#30DDD9;
 
}
.loginbar{
  margin-top:216rpx;
}
.loginbar button{
  width:90%;
  background: none;
  border:none;
  background:#30DDD9;
  font-size:36rpx;
  color:#ffffff;
  
}
.phone_huo {
  display: none;
}